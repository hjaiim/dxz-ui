{
    "name": "dxz-ui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "lib": "vue-cli-service build --target lib --dest lib packages/index.js",
        "new": "node scripts/new_components.js"
    },
    "dependencies": {
        "core-js": "^3.6.4",
        "vue": "^2.6.11",
        "vue-router": "^3.1.5"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.2.0",
        "@vue/cli-plugin-eslint": "~4.2.0",
        "@vue/cli-plugin-router": "~4.2.0",
        "@vue/cli-service": "~5.0.8",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-vue": "^6.1.2",
        "file-save": "^0.2.0",
        "markdown-it-container": "^2.0.0",
        "postcss-salad": "^2.0.1",
        "prettier": "^1.19.1",
        "sass": "^1.25.0",
        "sass-loader": "^8.0.2",
        "uppercamelcase": "^3.0.0",
        "vue-markdown-loader": "^2.4.1",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {},
            "postcss-salad": {
                "browsers": [
                    "ie > 8",
                    "last 2 versions"
                ],
                "features": {
                    "bem": {
                        "shortcuts": {
                            "component": "b",
                            "descendent": "e",
                            "modifier": "m"
                        },
                        "separators": {
                            "descendent": "__",
                            "modifier": "_"
                        }
                    }
                }
            }
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}